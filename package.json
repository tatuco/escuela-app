    "lint": "^0.7.0",
    "ts-node": "3.3.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/helmet": "0.0.42",
    "@types/jsonwebtoken": "^8.3.5",
    "aws-sdk": "^2.594.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bufferutil": "^4.0.1",
    "class-validator": "^0.9.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "express-jwt-blacklist": "^1.1.0",
    "faker": "^4.1.0",
    "helmet": "^3.21.2",
    "http": "0.0.0",
    "js-base64": "^3.5.2",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "moment": "^2.27.0",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.4.2",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.10",
    "request": "^2.88.2",
    "require-directory": "^2.1.1",
    "socket.io": "^2.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc": "^2.0.3",
    "turf": "^3.0.14",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.22",
    "typeorm-typedi-extensions": "^0.2.3",
    "underscore": "^1.9.2",
    "utf-8-validate": "^5.0.2",
    "xbee-api": "^0.6.0"
  },
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lint-s": "standard '*/**/*.{js,ts,tsx}' --fix",
    "start": "set NODE_ENV=production node ./build/index.js",
    "dev": "set NODE_ENV=dev && set debug=* && ts-node-dev --respawn --poll src/index.ts",
    "prod": "set NODE_ENV=production && tsc && node ./build/index.js",
    "server": "set NODE_ENV=production && tsc && node ./build/index.js && cp -r ./src/hardware ./src/logs ./build",
    "test": "set NODE_ENV=test && tsc && node ./build/index.js",
    "seed": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:create": "typeorm migration:create -n MigracionName",
    "migrations:generate": "ts-node ./node_modules/typeorm/cli.js migrations:generate",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:run:prod": "tsc ts-node ./node_modules/typeorm/cli.js migration:run"
  }
}
